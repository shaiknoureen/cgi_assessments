#operators
#1. Arithmetic operators

abc = 21
bcd = 4
print(abc % bcd)

print(abc ** bcd) #10x10x10x10x10

print(abc //bcd)


#assignment operators
x = 5
x+= 5# ==> x = x+5 =5+5 =10
# here x = x+5 = 5=5 = 10 here we are adding the x+x
x -= 5 #==> x = x-5 =5-5 =0

#comparision operator

a = 100
b = 50
print(a==b) #100== 50 False
print(a!=b) # 100!= 50 True
print(a>b)# 100>50 True
print(a<b)#100<50 False
print(a>=b)#100>=50 True
print(b<=a)#100<=50 True




print("apple"=="banana") #False
print("apple">"kiwi") #True
print("kiwi">="kiwi")#True


#logical operator
print(a >25 and a<125)
print(a >25 or a<25)
print(not(a>25 and a>125))
