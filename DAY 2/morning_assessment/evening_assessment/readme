# list operations
a = [1,2]
b = [3,4]
print(f"the addition of{a} and {b} is: ",a+b)
print(f"the multiplication of variable{a} with two is:",a*2)
print(f"to check whether the given element is in the variable {b}:",3 in b)
print(len(a))
# list methods
#sort()
xyz = [10,50,20,100,300,200,-1,-10,-5,-30]
xyz.sort()
print(xyz)

list_app = [1,2,3,4,5]
#append()
list_app.append(6)
print(list_app)

#insert()
accessories = ["phone","tv","powerbank","laptop"]
accessories.insert(1,"tab")
accessories.extend(["samsung","Apple"])
print(accessories)

#extend() vs append()
a1 = [1,2]
a1.append([3,4])
print(a1)
a2 = [1,2]
a2.extend([3,4])
print(a2)

