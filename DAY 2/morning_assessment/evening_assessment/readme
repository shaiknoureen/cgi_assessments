tuple_access = (10,20,30,40)
list_access = list(tuple_access)
print(list_access)
list_access[1] = 10
print(type(list_access))
tuple_access = tuple(list_access)
print(tuple_access)
print(type(tuple_access))


#swapping the numbers
a,b = 10,20
print("the value of a is:",a,"the value of the b is:",b)
a,b = b,a # a= b=20,b=a=10
print("the value of a is:",a,"the value of the b is:",b)


