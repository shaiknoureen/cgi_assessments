function add(a:number,b:number):number{
    return a+b;
    }
console.log("function output");
console.log(add(5,10),"\n");

class car{
    brand : string;
    year :number;
    constructor(brand:string,year:number){


     this.brand = brand;
     this.year= year;
     }
     display():void{
         console.log(` this is ${this.brand}-and the model is${this.year}`)
         }

   }
console.log("class output");
const mycar = new car("toyota",2020);
mycar.display();
console.log("");














